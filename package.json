{
  "name": "dsd-admin",
  "version": "1.0.0",
  "engines": {
    "node": "10.10.0"
  },
  "main": "index.js",
  "repository": "https://github.com/cmeaun-a/DSD",
  "author": "cmeaun-a",
  "license": "MIT",
  "scripts": {
    "dev:server": "yarn nodemon src/server/app.js",
    "dev:client": "webpack-dev-server",
    "db:create": "babel-node database/scripts/create.js",
    "db:drop": "babel-node database/scripts/drop.js",
    "db:dump": "babel-node database/scripts/dump.js",
    "db:load": "babel-node database/scripts/load.js",
    "db:migrate": "yarn knex migrate:latest",
    "db:migrate:make": "yarn knex migrate:make",
    "db:rollback": "yarn knex migrate:rollback",
    "db:trucate": "yarn db:drop && yarn db:create && yarn db:load && yarn db:migrate",
    "db:seed": "yarn db:trucate && knex seed:run",
    "db:init": "yarn db:create && yarn db:migrate && yarn db:seed",
    "build:client": "rm -rf public/dist && NODE_ENV=production webpack",
    "build:server": "rm -rf lib/ && cp -R src/ lib/ && NODE_ENV=production babel -d lib src",
    "build": "yarn build:client && yarn build:server",
    "heroku-postbuild": "yarn build",
    "start": "NODE_ENV=production node lib/server/app.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "apollo-boost": "^0.1.20",
    "apollo-client": "^2.5.1",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "browser-history": "^1.0.1",
    "cloudinary-react": "^1.0.6",
    "connect-redis": "^3.4.0",
    "convict": "^4.4.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-session": "^1.15.6",
    "final-form": "^4.12.0",
    "graphql": "^14.1.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "jsonwebtoken": "^8.5.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "mz": "^2.7.0",
    "objection": "^1.6.3",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
