    "query-string": "^6.3.0",
    "react": "^16.8.3",
    "react-apollo": "^2.5.1",
    "react-apollo-hooks": "^0.4.3",
    "react-dom": "^16.8.3",
    "react-final-form": "^3.6.7",
    "react-hot-loader": "^4.7.2",
    "react-router-dom": "^4.3.1",
    "redis": "^2.8.0",
    "styled-components": "^4.0.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
